<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Course Dashboard</h1>

            <div class="section group" ng-repeat="data in courses">
                <div class="col span_1_of_6">
                <strong class="strongFont">Major</strong>
                <p>{[{data.major}]}</p>
                </div>
                <div class="col span_1_of_6">
                <strong class="strongFont">Course</strong>
                <p>{[{data.course}]}</p>
                </div>
                <div class="col span_1_of_6">
                <strong class="strongFont">Rubric</strong>
                <p>{[{data.rubric.name}]}</p>
                </div>
                <div class="col span_3_of_6">
                <button ng-click="edit(data._id)" class="btn btn-danger float-left">Update / Audit course</button>
                </div>
            </div>
           

        </div>
    </div>
</div>