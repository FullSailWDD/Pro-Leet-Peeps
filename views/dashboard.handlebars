<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Course Dashboard</h1>

            <div class="section group" ng-repeat="data in courses">
                <div class="col span_1_of_6">
                {[{data.major}]}
                </div>
                <div class="col span_1_of_6">
                {[{data.course}]}
                </div>
                <div class="col span_1_of_6">
                {[{data.rubric.name}]}
                </div>
                <div class="col span_3_of_6">
                <button ng-click="audit(data._id)" class="btn btn-default float-left">Audit</button>
                <button ng-click="edit(data._id)" class="btn btn-danger float-left">Edit</button>
                <button ng-click="remove(data._id)" class="btn btn-success float-left">Remove</button>
                </div>
            </div>
            </div>

        </div>
    </div>
</div>