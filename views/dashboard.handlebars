<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Course Dashboard</h1>

        <table class="table">
          <thead>
            <tr>
                <th>Major</th> 
                <th>Course</th>        
                <th>Rubric</th>
                <th>View</th>
                <th>Edit</th>
                <th>Remove</th>
            </tr>
        </thead>

        <tr ng-repeat="data in courses">
                <td>{[{data.major}]}</td>
                <td>{[{data.course}]}</td>
                <td>{[{data.rubric.name}]}</td>
                <td><button ng-click="audit(data._id)" class="btn btn-default">Audit</button></td>
                <td><button ng-click="edit(data._id)" class="btn btn-danger">Edit</button></td>
                <td><button ng-click="remove(data._id)" class="btn btn-success">Remove</button></td>
        </tr>
    </table>
            </div>

        </div>
    </div>
</div>
