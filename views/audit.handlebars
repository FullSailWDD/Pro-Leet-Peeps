<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Audit your course</h1>
                <input type="text" value="WDD">


                </input>
                <div ng-repeat="data in courses">
                        <p>{[{data.major}]}</p>
                        <p>{[{data.course}]}</p>
                        <p>{[{data.rubric.name}]}</p>
                        <p>{[{data.rubric.grade}]}</p>
                        <p><button ng-click="edit(data._id)" class="btn btn-danger">Update grade</button></p>
                </div>
            </div>

        </div>
    </div>
</div>